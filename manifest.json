{
  "manifest_version": 2,

  "name": "Github Markdown Word Count",
  "description": "Counts words in a Github Markdown page render",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["jquery-2.1.3.min.js", "script.js"]
    }
  ]
}